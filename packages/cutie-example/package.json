{
  "name": "@openstax/cutie-example",
  "description": "Example application demonstrating QTI v3 assessment items",
  "version": "1.0.0",
  "dependencies": {
    "@openstax/cutie-client": "1.0.0",
    "@openstax/cutie-core": "1.0.0",
    "@openstax/cutie-editor": "1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "cspell": "^5.18.5",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
